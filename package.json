{
  "name": "react-boilerplate",
  "version": "4.0.0",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/react-boilerplate/react-boilerplate.git"
  },
  "engines": {
    "yarn": ">=1.19",
    "node": ">=8.15.1"
  },
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "yarn run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "node ./internals/scripts/extract-intl.js",
    "prebuild": "yarn run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "yarn run test && yarn run build && yarn run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "presetup": "yarn add chalk shelljs compare-versions --dev",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "node ./internals/scripts/clean.js",
    "clean:all": "yarn run analyze:clean && yarn run test:clean && yarn run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:css": "stylelint app/**/*.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "yarn run lint:eslint . ",
    "pretest": "yarn run test:clean && yarn run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn run lint:eslint:fix",
      "git add --force"
    ],
    "*.{md,json}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@formatjs/intl-pluralrules": "1.2.0",
    "@formatjs/intl-relativetimeformat": "2.8.0",
    "chalk": "2.4.2",
    "compression": "1.7.4",
    "connected-react-router": "6.5.2",
    "cross-env": "6.0.3",
    "express": "4.17.1",
    "fontfaceobserver": "2.1.0",
    "history": "4.10.1",
    "hoist-non-react-statics": "3.3.0",
    "immer": "4.0.2",
    "intl": "1.2.5",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "lodash": "4.17.15",
    "minimist": "1.2.0",
    "offline-plugin": "5.0.7",
    "prop-types": "15.7.2",
    "react": "16.11.0",
    "react-app-polyfill": "1.0.4",
    "react-dom": "16.11.0",
    "react-helmet-async": "1.0.4",
    "react-intl": "2.9.0",
    "react-redux": "7.1.1",
    "react-router-dom": "5.1.2",
    "redux": "4.0.4",
    "redux-injectors": "1.2.0",
    "redux-saga": "1.1.1",
    "reselect": "4.0.0",
    "sanitize.css": "11.0.0",
    "styled-components": "4.4.0"
  },
  "devDependencies": {
    "@babel/cli": "7.6.4",
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.6.0",
    "@babel/plugin-transform-react-constant-elements": "7.6.3",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "@babel/register": "7.6.2",
    "@testing-library/jest-dom": "4.2.0",
    "@testing-library/react": "9.3.0",
    "add-asset-html-webpack-plugin": "3.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-dynamic-import-node": "2.3.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-react-intl": "4.1.21",
    "babel-plugin-styled-components": "1.10.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "circular-dependency-plugin": "5.2.0",
    "compare-versions": "3.5.1",
    "compression-webpack-plugin": "3.0.0",
    "coveralls": "3.0.7",
    "css-loader": "3.2.0",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.4.0",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "eslint-plugin-redux-saga": "1.1.1",
    "file-loader": "4.2.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.9",
    "image-webpack-loader": "6.0.0",
    "imports-loader": "0.8.0",
    "jest-cli": "24.9.0",
    "jest-styled-components": "6.3.3",
    "jest-watch-typeahead": "0.4.0",
    "lint-staged": "9.4.2",
    "ngrok": "3.2.5",
    "node-plop": "0.22.0",
    "null-loader": "3.0.0",
    "plop": "2.5.2",
    "prettier": "1.18.2",
    "react-test-renderer": "16.11.0",
    "rimraf": "3.0.0",
    "shelljs": "0.8.3",
    "style-loader": "1.0.0",
    "stylelint": "11.1.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.8.0",
    "svg-url-loader": "3.0.2",
    "terser-webpack-plugin": "2.2.1",
    "url-loader": "2.2.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.9",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-pwa-manifest": "4.1.1",
    "whatwg-fetch": "3.0.0"
  }
}
